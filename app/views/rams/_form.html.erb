<%= form_with model: @ram do |form| %>
    <div>
        <%= form.label :brand %>
        <%= form.text_field :brand %>
    </div>

    <div>
        <%= form.label :model %>
        <%= form.text_field :model %>
    </div>

    <div>
        <%= form.label :size %>
        <%= form.number_field :size %>
    </div>

    <div>
        <%= form.label :ram_type %>
        <%= form.text_field :ram_type %>
    </div>

    <div>
        <%= form.label :speed %>
        <%= form.number_field :speed %>
    </div>

    <div>
        <%= form.label :dimm_type %>
        <%= form.text_field :dimm_type %>
    </div>

    <div>
        <%= form.label :cas_latency %>
        <%= form.text_field :cas_latency %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>