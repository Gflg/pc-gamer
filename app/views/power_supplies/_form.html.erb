<%= form_with model: @power_supply do |form| %>
    <div>
        <%= form.label :brand %>
        <%= form.text_field :brand %>
    </div>

    <div>
        <%= form.label :model %>
        <%= form.text_field :model %>
    </div>

    <div>
        <%= form.label :power %>
        <%= form.number_field :power %>
    </div>

    <div>
        <%= form.label :efficiency %>
        <%= form.text_field :efficiency %>
    </div>

    <div>
        <%= form.label :color %>
        <%= form.text_field :color %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>